  {
   "cell_type": "code",
   "execution_count": 54,
   "id": "0d2d8924-1507-4f75-bd11-22e29e5e4969",
   "metadata": {},
   "outputs": [],
   "source": [
    "def get_job_description(url):\n",
    "    response = requests.get(url)\n",
    "    soup = BS(response.text)\n",
    "    desc = soup.find('div', class_='content').find('p').text.strip()\n",
    "    return desc\n"
   ]
  },
