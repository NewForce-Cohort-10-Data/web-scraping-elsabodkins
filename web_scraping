   "source": [
    "def get_job_description(url):\n",
    "    try:\n",
    "        response = requests.get(url)\n",
    "        soup = BS(response.text)\n",
    "        desc = soup.find('div', class_='content').find('p').text.strip()\n",
    "        return desc\n",
    "    except Exception as e:\n",
    "        print(f\"Error fetching description from {url}: {e}\")\n",
    "        return None"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 68,
   "id": "15dcf5c0-7843-47b3-b520-804985a4e2a2",
   "metadata": {},
   "outputs": [],
   "source": [
    "df['Job Description'] = df['Apply URL'].apply(get_job_description)"
   ]
