   "outputs": [],
   "source": [
    "base_url = 'https://realpython.github.io'\n",
    "\n",
    "constructed_urls = []\n",
    "\n",
    "for i, title in enumerate(job_titles):\n",
    "    # Clean the title\n",
    "    cleaned = title.lower()\n",
    "    cleaned = cleaned.replace(',', '').replace('(', '').replace(')', '')\n",
    "    cleaned = '-'.join(cleaned.split())  # Replace spaces with hyphens\n",
    "    full_url = f\"{base_url}/fake-jobs/jobs/{cleaned}-{i}.html\"\n",
    "    constructed_urls.append(full_url)\n"
   ]
  },
